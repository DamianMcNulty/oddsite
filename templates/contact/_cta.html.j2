{% if not config.hide_contact %}
<div class="separator"></div>
<section data-module="contact-cta">
  <h2>
    {% if ('susysite' in config.location) %}
      {{ config.contact[0].susytitle }}
    {% else %}
      {{ config.contact[0].title }}
    {% endif %}
  </h2>
  <p>
    {{ config.contact[0].content|safe }}
  </p>
</section>
{% endif %}
